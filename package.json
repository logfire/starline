{
  "name": "starline",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write -- .",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "cf-typegen": "wrangler types --include-runtime false"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 119,
    "bracketSpacing": true,
    "plugins": [
      "prettier-plugin-toml"
    ]
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.7.5",
    "@cloudflare/workers-types": "^4.20250321.0",
    "typescript": "^5.5.2",
    "wrangler": "^4.4.0"
  },
  "dependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-toml": "^2.0.2"
  }
}
